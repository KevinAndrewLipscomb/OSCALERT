create user if not exists 'ki-oscalert'@'localhost' identified by '~ki-oscalert-password~';
grant  select
 , insert
 , update
 , delete
 , execute
 , create
 , alter
 , create routine
 , alter routine
  on oscardotnetdb.*
  to 'ki-oscalert'@'localhost'
;
--
-- Must grant PROCESS priv to primary user to include SHOW ENGINE INNODB
-- STATUS output in MySQL-related exception reports.
--
;
grant process
  on *.*
  to 'ki-oscalert'@'localhost'