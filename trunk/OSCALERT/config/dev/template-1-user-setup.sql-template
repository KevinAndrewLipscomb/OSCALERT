create user if not exists 'ki-oscalert-d'@'localhost' identified by '~';
grant  select
 , insert
 , update
 , delete
 , execute
 , create
 , alter
 , create routine
 , alter routine
  on oscardotnetdb_d.*
  to 'ki-oscalert-d'@'localhost'
;
--
-- Must grant PROCESS priv to primary user to include SHOW ENGINE INNODB
-- STATUS output in MySQL-related exception reports.
--
;
grant process
  on *.*
  to 'ki-oscalert-d'@'localhost'